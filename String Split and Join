"""
Problem: Splitting a string and joining it with hyphen(s)
Platform: HackerRank
Language: Pypy3
Difficulty: Easy
"""
def split_and_join(line):
    new_line=line.split(" ")
    final_line='-'.join(new_line)
    return final_line
if __name__ == '__main__':
    line = input()
    result = split_and_join(line)
    print(result)
